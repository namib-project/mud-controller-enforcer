# OpenSSL configuration file for creating a CSR for a server certificate

[ req ]
default_bits = 4096
default_md = sha256
prompt = no
encrypt_key = no
distinguished_name = dn
req_extensions = req_ext
x509_extensions = req_ext
utf8 = yes

[ dn ]
C = DE
ST = Bremen
L = Bremen
O = Uni Bremen
OU = NAMIB
CN = _authority._namib

[ req_ext ]
basicConstraints = CA:TRUE

